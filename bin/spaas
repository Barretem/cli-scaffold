#! /usr/bin/env node

const program = require('commander')
const { getPkgVersion, printPkgVersion } = require('../dist/utils')

printPkgVersion()

program
  .version(getPkgVersion())
  .usage('<command> [options]')
  .command('add [packages ...] [flags]', 'Add private packages of spaas')
  .parse(process.argv)
